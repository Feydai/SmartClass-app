{
  "extends": "@react-native/typescript-config",
  "compilerOptions": {
  "jsx": "react-native",
  "types": ["react"],
  "strict": true,
  "lib": ["dom", "esnext"],
  "paths": {
    "@/*": ["./*"],
  }
},
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
